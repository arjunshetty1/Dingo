{
  "manifest_version": 2,
  "name": "DINGO",
  "version": "1.0",
  "description": "Redirects to Twitter when trying to access Instagram Reels",
  "permissions": ["webRequestBlocking", "webRequest", "*://*.instagram.com/*"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "reelink.png"
  }
}
